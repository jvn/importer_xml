<h1>Listing import_jobs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Customer</th>
    <th>Filename</th>
    <th>Status</th>
    <th>Konv</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @import_jobs.each do |import_job| %>
  <tr>
    <td><%= import_job.name %></td>
    <td><%= import_job.customer %></td>
    <td><%= import_job.filename %></td>
    <td><%= import_job.status %></td>
    <td><%= import_job.konv_id %></td>
    <td><%= link_to 'Show', import_job %></td>
    <td><%= link_to 'Edit', edit_import_job_path(import_job) %></td>
    <td><%= link_to 'Destroy', import_job, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

